<body id="myPage">
    <nav class="navbar navbar-default navbar-fixed-top">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#nav-camp" aria-expanded="false">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          </button>
          <a href="/" class="navbar-brand">
            <img class="logo-img" src="/img/ge_logo.png">
            <span class="logo"> EQ Wiki</span>
          </a>
        </div>
        <div class="collapse navbar-collapse" id="nav-camp">
          <ul class="nav navbar-nav">
            <li class="divider-vertical"></li>
          </ul>
          <ul class="nav navbar-nav navbar-right">
            <% if(!currentUser){ %>
              <li><a href="/login">Login</a></li>
              <li><a href="/register">Sign Up</a></li>
            <% } else  if (currentUser.role != 'manager') { %>
                  <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="true"><%= currentUser.username %>  <span class="badge"><%= currentUser.ranking %></span> <span class="caret"></span></a>
                    <ul class="dropdown-menu" role="menu">
                      <li><a href="/posts/new">Create new post</a></li>
                      <li><a href="/tags/new">Add new tags</a></li>
                      <li class="divider"></li>
                      <li><a href="/profile/<%= currentUser._id %>">Profile Page</a></li>
                      <li><a href="#">Settings</a></li>
                    </ul>
                  </li>
                  <li><a href="/logout">Logout</a></li>
            <% } else { %>
                  <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="true"><%= currentUser.username %>  <span class="badge"><%= currentUser.ranking %></span> <span class="caret"></span></a>
                    <ul class="dropdown-menu" role="menu">
                      <li><a href="/posts/new">Create new post</a></li>
                      <li><a href="/tags/new">Add new tags</a></li>
                      <li class="divider"></li>
                      <li><a href="/profile/<%= currentUser._id %>">Profile Page</a></li>
                      <li><a href="/admin/<%= currentUser._id %>">Manager Page</a></li>
                      <li><a href="#">Settings</a></li>
                    </ul>
                  </li>
                  <li><a href="/logout">Logout</a></li>
            <% } %>
          </ul>
        </div>
      </div>
    </nav>